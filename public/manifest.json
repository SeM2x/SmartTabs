{
  "manifest_version": 3,
  "name": "My Vite Extension",
  "version": "1.0",
  "description": "A basic Chrome extension",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
}